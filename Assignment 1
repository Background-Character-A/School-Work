#include <stdio.h>

void a();
void b();
void c();
void d();

int main()
{
    a();
    printf("\n");
    b();
    printf("\n");
    c();
    printf("\n");
    d();

    return 0;
}

void a()
{
    printf("a.");
    for (int i = 1; i < 10; i += 2)
    {
        printf("%d ", i);
    }

    return;
}

void b()
{
    printf("b.");
    for (int i = 9; i > 0; i -= 2)
    {
        printf("%d ", i);
    }

    return;
}

void c()
{
    printf("c.");
    for (int i = 10; i <= 100; i += 10)
    {
        printf("%d ", i);
    }

    return;
}

void d()
{
    printf("d.");
    for (int i = 1; i <= 5; i++)
    {
        for (int j = 1; j <= i; j++)
        {
            printf("%d ", j);
        }
        
        printf("\n");

    
    }
    return;
}
